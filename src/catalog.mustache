    {{> templates/screens/07-home-partners-screen }}
    {{> templates/screens/09-home-contacts-screen }}
    {{> templates/inner-pages/team-footer }}
    <div class="popup" id="popup-1">
        <div class="container">
            <div class="inner">
                <a href="#" class="close-popup">x</a>
                <div class="row">
